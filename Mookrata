        // JavaScript to handle airdrop participation and notifications

        document.getElementById('airdropButton').addEventListener('click', function() {
            const alertBox = document.getElementById('alertBox');
            alertBox.textContent = 'Coming soon.';
            alertBox.style.display = 'block';
        });

        // Function to select a random winner from a list of participants
        function selectRandomWinner(participants) {
            if (participants.length === 0) return 'No participants available';
            const randomIndex = Math.floor(Math.random() * participants.length);
            return participants[randomIndex];
        }
